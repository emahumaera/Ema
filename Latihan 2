#include<iostream>

using namespace std;

int main () {
    cout << "A OR B :" << endl;
    cout << " 0 || 0 = " << ( 0 || 0 ) << endl;
    cout << " 0 || 0 = " << ( 0 || 0 ) << endl;
    cout << " 0 || 1 = " << ( 0 || 1 ) << endl;
    cout << " 0 || 1 = " << ( 0 || 1 ) << endl;
    cout << " 1 || 0 = " << ( 1 || 0 ) << endl;
    cout << " 1 || 0 = " << ( 1 || 0 ) << endl;
    cout << " 1 || 1 = " << ( 1 || 1 ) << endl;
    cout << " 1 || 1 = " << ( 1 || 1 ) << endl;
    cout << "A OR C :" << endl;
    cout << " 0 || 0 = " << ( 0 || 0) << endl;
    cout << " 0 || 1 = " << ( 0 || 1) << endl;
    cout << " 0 || 0 = " << ( 0 || 0) << endl;
    cout << " 0 || 1 = " << ( 0 || 1) << endl;
    cout << " 1 || 0 = " << ( 1 || 0) << endl;
    cout << " 1 || 1 = " << ( 1 || 1) << endl;
    cout << " 1 || 0 = " << ( 1 || 0) << endl;
    cout << " 1 || 1 = " << ( 1 || 1) << endl;
    cout << "F1= (A OR B) AND (A OR c) :" << endl;
    cout <<" 0 && 0 = " << ( 0 && 0) << endl;
    cout <<" 0 && 1 = " << ( 0 && 1) << endl;
    cout <<" 1 && 0 = " << ( 1 && 0) << endl;
    cout <<" 1 && 1 = " << ( 1 && 1) << endl;
    cout <<" 1 && 1 = " << ( 1 && 1) << endl;
    cout <<" 1 && 1 = " << ( 1 && 1) << endl;
    cout <<" 1 && 1 = " << ( 1 && 1) << endl;
    cout <<" 1 && 1 = " << ( 1 && 1) << endl;


    return 0;
}
